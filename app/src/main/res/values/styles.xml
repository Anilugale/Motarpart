<resources xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <item name="android:actionBarStyle">@style/MyActionBar</item>
        <item name="colorPrimary">#fff</item>
        <item name="colorPrimaryDark">#ffbababa</item>

    </style>
    <style name="MyActionBar" parent="@style/Theme.AppCompat.Light">
        <item name="android:icon">@drawable/iconl</item>

        <item name="android:homeAsUpIndicator">@drawable/home</item>
        <item name="android:titleTextStyle">@style/NoTitleText</item>
        <item name="android:subtitleTextStyle">@style/NoTitleText</item>
    </style>


    <style name="NoTitleText" parent="@style/Theme.AppCompat.Light">
        <item name="android:textSize">0sp</item>
        <item name="android:textColor">#00000000</item>
    </style>
</resources>
